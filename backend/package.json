{
  "name": "botbattle-backend",
  "version": "1.0.0",
  "description": "The backend code for the botbattle website",
  "main": "server.ts",
  "prisma.schema": "./models/schema.prisma",
  "scripts": {
    "test": "vitest",
    "test:ui": "vitest --ui",
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "nodemon --watch . --ext ts,js --exec ts-node server.ts",
    "makemigrations": "npx prisma migrate dev --create-only",
    "migrate": "npx prisma migrate deploy && npx prisma generate"
  },
  "repository": {
    "type": "git",
    "url": "https://git.bitscuit.be/bitscuit/BotBattle-Website.git"
  },
  "author": "",
  "license": "GPL-3.0-or-later",
  "dependencies": {
    "@prisma/client": "^6.9.0",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-session": "^1.18.1",
    "express-ws": "^5.0.2",
    "helmet": "^8.1.0",
    "morgan": "^1.10.0",
    "session-file-store": "^1.5.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/express-session": "^1.18.2",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.0.0",
    "@types/session-file-store": "^1.2.5",
    "@types/supertest": "^6.0.3",
    "@vitest/ui": "^3.2.4",
    "nodemon": "^3.1.10",
    "prisma": "^6.9.0",
    "supertest": "^7.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  }
}
